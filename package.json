  },
  "devDependencies": {
    "@rdfjs/data-model": "github:rdfjs/data-model",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.34",
    "@types/rdf-dataset-indexed": "^0.4.5",
    "@types/rdf-js": "^4.0.1",
    "@types/rdfjs__namespace": "^1.1.3",
